//lower bound
private static int LowerBound(int[][] matrix)
{
	int min_degree=0;
	boolean first=true;
	for(int i=0; i<matrix.length; i++)
	{
		int degree=0;
		for(int j=0; j<matrix[i].length; j++)
		{
		degree=degree+matrix[i][j];
		}
	if(first && degree!=0){min_degree=degree;
	first=false;}
	else if (degree<min_degree && degree!=0){min_degree=degree;}
	}
	return min_degree;
}
